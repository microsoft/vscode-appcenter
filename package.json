{
    "name": "vscode-appcenter",
    "displayName": "VSCode AppCenter Tools",
    "description": "AppCenter extension",
    "version": "0.0.1",
    "publisher": "vsmobile",
    "engines": {
        "vscode": "^1.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-appcenter.git"
    },
    "bugs": {
        "url": "https://github.com/Microsoft/vscode-appcenter/issues"
    },
    "homepage": "https://github.com/Microsoft/vscode-appcenter/blob/master/README.md",
    "categories": [
        "Other"
    ],
    "keywords": [
        "app-center"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "appcenter.whoami",
                "title": "Who Am I",
                "category": "AppCenter"
            },
            {
                "command": "appcenter.login",
                "title": "Login",
                "category": "AppCenter"
            },
            {
                "command": "appcenter.logout",
                "title": "Logout",
                "category": "AppCenter"
            },
            {
                "command": "appcenter.menu",
                "title": "Show Menu",
                "category": "AppCenter"
            },
            {
                "command": "appcenter.start",
                "title": "Start An Idea",
                "category": "AppCenter"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "AppCenter configuration",
            "properties": {
                "appcenter.loginendpoint": {
                "description": "Endpoint to login to appcenter",
                    "type": "string",
                    "default": "https://appcenter.ms/cli-login",
                    "scope": "resource"
                },
                "appcenter.api.endpoint": {
                    "description": "API Endpoint to appcenter",
					"type": "string",
					"default": "https://api.appcenter.ms",
					"scope": "resource"
                },
                "appcenter.api.distribitiongrouptestersname": {
                    "description": "AppCenter Distribution Group Testers Name",
					"type": "string",
					"default": "Beta Testers",
					"scope": "resource"
                },
                "appcenter.api.createtestersdistibutiongroup": {
                    "description": "Configure if we should create new testers distribution group",
					"type": "boolean",
					"default": "true",
					"scope": "resource"
                },
                "appcenter.api.connectrepotobuildservice": {
                    "description": "Configure if we should connect repo to build service",
					"type": "boolean",
					"default": "true",
					"scope": "resource"
                },
                "appcenter.api.configurebranchandstartnewbuild": {
                    "description": "Configure branch and start new build",
					"type": "boolean",
					"default": "true",
					"scope": "resource"
                },
                "appcenter.api.defaultbranchname": {
                    "description": "AppCenter Default Branch Name",
					"type": "string",
					"default": "master",
					"scope": "resource"
                },
                "appcenter.api.createiosapp": {
                    "description": "Create iOS App In AppCenter",
					"type": "boolean",
					"default": "true",
					"scope": "resource"
                },
                "appcenter.api.createandroidapp": {
                    "description": "Create Android App In AppCenter",
					"type": "boolean",
					"default": "true",
					"scope": "resource"
                },
                "appcenter.logLevel": {
					"description": "Logging level in extension",
					"type": "enum",
					"enum": [
						"Trace",
						"Debug",
						"Info",
						"Warning",
						"Error",
						"None"
					],
					"default": "Info",
					"scope": "resource"
				}
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "gulp",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "tslint --project tsconfig.json -e src/*.d.ts -t verbose",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "chalk": "^2.3.2",
        "date-fns": "^1.29.0",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^4.0.1",
        "jsonwebtoken": "^8.2.0",
        "lodash": "^4.17.5",
        "mkdirp": "^0.5.1",
        "moment": "^2.21.0",
        "ms-rest": "^2.3.2",
        "open": "0.0.5",
        "opener": "^1.4.3",
        "run-sequence": "^2.2.1",
        "rx-lite": "^4.0.8",
        "temp": "^0.8.3",
        "tslint": "^5.9.1",
        "tslint-microsoft-contrib": "^5.0.3",
        "xml2js": "^0.4.19"
    }
}
